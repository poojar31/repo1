pipeline {
  agent any;
   stages{
     stage ('BUILD') {
       steps {
         sh "sleep 5"
         echo "This is BUILD stage"
       }
     }
     stage ('TEST') {
       steps {
         sh "sleep 5"
         echo "This is TEST stage"
       }
     }
     stage ('DEPLOY') {
       steps {
         sh "sleep 5"
         echo "This is DEPLOY stage"
       }
     }
   }
}
